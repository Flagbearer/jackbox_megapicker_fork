{
  "name": "boxbox",
  "version": "0.4.1-2",
  "private": true,
  "ciTrigger": 4,
  "license": "UNLICENSED",
  "productName": "The Jackbox Megapicker",
  "description": "The Jackbox Megapicker is your all-in-one solution for organizing, buying, and launching all of your Jackbox games.",
  "copyright": "Copyright Â© 2024 Jackbox Games",
  "author": "Jackbox Games <jbg@jackboxgames.com> (https://jackboxgames.com)",
  "contributors": [
    "Erin Bleiweiss <ebleiweiss@jackbox.com >",
    "PJ Conde <pj@jackboxgames.com>",
    "Chase McClure <chase@jackboxgames.com>",
    "Eris Price <eris@jackboxgames.com>",
    "Brad Vah <brad@jackboxgames.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackboxgames/boxbox.git"
  },
  "main": ".vite/build/main.js",
  "scripts": {
    "setup": "(op inject -f -i .env.tmpl -o .env && op inject -f -i .env.build.tmpl -o .env.build) || echo \"You may need to set the OP_VAULT=eng-uat environment variable\"",
    "start": "electron-forge start -- -config devMenu=true",
    "package:linux": "electron-forge package --platform=linux --arch=x64",
    "package:mac": "electron-forge package --platform=darwin --arch=universal",
    "package:mac:arm": "electron-forge package --platform=darwin --arch=arm64",
    "package:mac:x64": "electron-forge package --platform=darwin --arch=x64",
    "package:windows": "electron-forge package --platform=win32",
    "package:windows:arm": "electron-forge package --platform=win32 --arch=arm64",
    "package:windows:x64": "electron-forge package --platform=win32 --arch=x64",
    "make:linux": "electron-forge make --platform=linux --arch=x64",
    "make:mac": "electron-forge make --platform=darwin --arch=universal",
    "make:mac:arm": "electron-forge make --platform=darwin --arch=arm64",
    "make:windows": "electron-forge make --platform=win32",
    "schema": "npm run schema:home && npm run schema:games && npm run schema:products",
    "schema:home": "npx typescript-json-schema tsconfig.json BoxBox.HomeJSON --include src/shared/types/BoxBox.ts --out schema.home.json",
    "schema:games": "npx typescript-json-schema tsconfig.json BoxBox.GamesJSON --include src/shared/types/BoxBox.ts --out schema.games.json",
    "schema:products": "npx typescript-json-schema tsconfig.json BoxBox.ProductsJSON --include src/shared/types/BoxBox.ts --out schema.products.json",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "vitest"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-vite": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@jackboxgames/eslint-config": "^1.10.0",
    "@types/mixpanel": "^2.14.8",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/ui": "^1.6.0",
    "electron": "^31.2.0",
    "sass": "^1.72.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "typescript-json-schema": "^0.65.1",
    "vite": "^5.3.3",
    "vite-plugin-vue-devtools": "^7.4.4",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@jackboxgames/filter": "^1.1.2",
    "@node-steam/vdf": "^2.2.0",
    "@sentry/electron": "^4.22.0",
    "debounce": "^2.0.0",
    "dotenv": "^16.4.5",
    "electron-log": "^5.1.5",
    "i18next": "^23.11.2",
    "i18next-vue": "^4.0.0",
    "mixpanel": "^0.18.0",
    "openid": "^2.0.12",
    "slugify": "^1.6.6",
    "steamworks.js": "^0.3.2",
    "throttleit": "^2.1.0",
    "vue": "^3.4.21"
  }
}
